{
  "name": "xrpl-trading-bot",
  "version": "2.0.0",
  "description": "Modular XRPL trading bot with sniper and copy trading capabilities",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:sniper": "node dist/index.js --sniper",
    "start:copy": "node dist/index.js --copy",
    "start:both": "node dist/index.js",
    "dev": "ts-node index.ts",
    "dev:watch": "nodemon --exec ts-node index.ts"
  },
  "keywords": [
    "xrpl",
    "trading",
    "bot",
    "sniper",
    "copy-trading",
    "amm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/cryptojs": "^3.1.33",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "cryptojs": "^2.5.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "node-cron": "^4.0.7",
    "xrpl-api": "^3.7.21",
    "uuid": "^11.1.0",
    "xrpl": "^4.2.5",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
