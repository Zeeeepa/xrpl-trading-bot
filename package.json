{
    "name": "xrpl-trading-bot",
    "version": "2.0.0",
    "description": "Modular XRPL trading bot with sniper and copy trading capabilities",
    "main": "index.ts",
    "scripts": {
      "build": "tsc",
      "start": "ts-node index.ts",
      "start:sniper": "ts-node index.ts --sniper",
      "start:copy": "ts-node index.ts --copy",
      "start:both": "ts-node index.ts",
      "dev": "ts-node index.ts",
      "dev:watch": "nodemon --exec ts-node index.ts"
    },
    "keywords": [
      "xrpl",
      "trading",
      "bot",
      "sniper",
      "copy-trading",
      "amm"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
      "dotenv": "^16.5.0",
      "xrpl": "^4.2.5",
      "ws": "^8.18.0",
      "ts-node": "^10.9.2",
      "typescript": "^5.3.3",
      "flip-prx": "^1.0.2"
    },
    "devDependencies": {
      "@types/node": "^20.10.0",
      "@types/ws": "^8.5.10",
      "nodemon": "^3.1.10"
    },
    "engines": {
      "node": ">=16.0.0"
    }
  }